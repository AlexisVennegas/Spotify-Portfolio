---
import ArticleModule from './ArticleModule.astro'
interface Props {
    title: string;
}
const { title } = Astro.props;

---

<section
transition:name=`Article${title}`
>
    <ArticleModule title="Contacto" />
    <ArticleModule title="Habilidades" />
    <ArticleModule title="Conoceme" />
    <ArticleModule title="Proyectos" />
    <ArticleModule title="Educacion" />
    <ArticleModule title="Nasa" />


</section>

<style>
  section {
    width: 100%;
    height: 85%;
    display: flex;
    flex-wrap: wrap;
    gap: 1rem;
    overflow-y: auto;

}
</style>